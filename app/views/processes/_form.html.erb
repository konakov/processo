<%= form_for @process do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :goal %><br />
    <%= f.text_area :goal %>
  </p>
  <p>
    <%= f.label :person_id %><br />
    <%= f.number_field :person_id %>
  </p>
  <p>
    <%= f.label :organization_id %><br />
    <%= f.number_field :organization_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
